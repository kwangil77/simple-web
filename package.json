{
  "name": "simple-web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "openapi:download": "mkdir -p ./schema && curl -sq https://apicurio-registry.example.io/apis/registry/v2/groups/simple/artifacts/simple-rest -o ./schema/simple-rest.json",
    "proto:download": "mkdir -p ./schema && curl -sq https://apicurio-registry.example.io/apis/registry/v2/groups/simple/artifacts/simple-grpc -o ./schema/simple-grpc.proto",
    "graphql:download": "mkdir -p ./schema && curl -sq https://apicurio-registry.example.io/apis/registry/v2/groups/simple/artifacts/simple-gql -o ./schema/simple-gql.graphql",
    "openapi:generate": "mkdir -p ./openapi && openapi -i ./schema/simple-rest.json -c axios -o ./openapi/simple-rest --name ApiClient",
    "proto:generate": "mkdir -p ./proto && grpc_tools_node_protoc --plugin=./node_modules/.bin/protoc-gen-ts_proto --ts_proto_out=./proto --ts_proto_opt=outputServices=grpc-js,env=node,esModuleInterop=true --proto_path=./schema ./schema/simple-grpc.proto",
    "graphql:generate": "graphql-codegen --config simple-gql.ts"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.9.7",
    "@opentelemetry/api": "^1.4.1",
    "@opentelemetry/auto-instrumentations-node": "^0.39.4",
    "@opentelemetry/sdk-trace-node": "^1.15.2",
    "@tanstack/react-query": "^4.36.1",
    "axios": "^1.5.1",
    "form-data": "^4.0.0",
    "graphql": "^16.8.1",
    "graphql-request": "^4.0.0",
    "graphql-scalars": "1.22.4",
    "next": "13.5.6",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@babel/traverse": "7.23.2",
    "@graphql-codegen/cli": "5.0.0",
    "@graphql-codegen/client-preset": "4.1.0",
    "@types/node": "18.11.9",
    "@types/react": "18.2.31",
    "@types/react-dom": "18.2.14",
    "autoprefixer": "10.4.16",
    "eslint": "8.52.0",
    "eslint-config-next": "13.5.6",
    "grpc-tools": "^1.12.4",
    "openapi-typescript-codegen": "^0.25.0",
    "postcss": "8.4.31",
    "tailwindcss": "3.3.3",
    "ts-proto": "^1.162.1",
    "typescript": "5.2.2",
    "typescript-graphql-request": "^4.4.6"
  },
  "packageManager": "yarn@4.1.1"
}
